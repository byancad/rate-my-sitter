<body>
    <a href="/signin" class="myButton">Sign Out</a>


    <div class="datetime">
        <p></p>
        <p id="datetime"></p>
    </div>
    <div class="row">
        <div class="col-md-12 note-container"></div>
    </div>
    <div class="container2">
        <div class="row mt-4">
            <div class="col-md-7">
                <div class="form-group">
                    <select class="custom-select" id="category">
                        <option selected value="">Choose A Category</option>
                        <option value="Personal">Personal</option>
                        <option value="Work">Work</option>
                        <option value="Family">Family</option>
                    </select>
                </div>
            </div>
            <div class="col-md-3"></div>
            <div class="col-md-2">
                <a class="btn btn-light" href="/review">Leave a Review!</a>
            </div>
        </div>
    </div>
    <!-- Custom Script -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.17.0/moment.min.js" type="text/javascript"></script>
